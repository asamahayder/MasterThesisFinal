{
  "data_load": {
    "params": {
      "path_to_data_folder": "../Data/Ex-Vivo/"
    }
  },
  "preprocessing": {
    "params": {}
  },
  "feature_engineering": {
    "params": {}
  },
  "model_training_and_evaluation": {
    "params": {
      "pca_required_variance_explained": {
        "min": 0.8,
        "max": 0.99
      },
      "degree_of_polynomial": {
        "min": 3,
        "max": 23
      },
      "k_outer": 4,
      "k_inner": 4,
      "C": {
        "min": 0.000001,
        "max": 1000000
      },
      "kernel": [
        "linear",
        "poly",
        "rbf",
        "sigmoid"
      ],
      "gamma": [
        "scale",
        "auto"
      ],
      "degree": {
        "min": 1,
        "max": 10
      },
      "coef0": {
        "min": -1,
        "max": 1
      },
      "tol": {
        "min": 0.000001,
        "max": 0.1
      },
      "optuna_n_trials": 30
    }
  }
}